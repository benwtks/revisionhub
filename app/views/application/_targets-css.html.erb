<style data-no-turbolinks="true">
  <% subjects.each do |subject| %> 
    @keyframes target-load-<%= subject.hyphenated_name %> {
      from {
        stroke-dashoffset: 400;
      }
      to {
        stroke-dashoffset: <%= subject.target_amount_to_offset * 324.75 %>;
      }
    }
  <% end %>
</style>
